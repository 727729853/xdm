<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="CategoryForegroundNormal" Color="#808080"/>
    <SolidColorBrush x:Key="CategoryForegroundSelected" Color="White"/>
    <SolidColorBrush x:Key="CategoryHighlight" Color="#0A6AB6"/>
    <SolidColorBrush x:Key="CategoryListBackground" Color="#24292E"/>
    
    <SolidColorBrush x:Key="ToolButtonForecolor" Color="#828282"/>
    <SolidColorBrush x:Key="ToolButtonMouseOverBackcolor" Color="#E6E6E6"/>
    <SolidColorBrush x:Key="ToolButtonMousePressedBackcolor" Color="#C8C8C8"/>
    <SolidColorBrush x:Key="ToolButtonMouseDisabledForecolor" Color="#B4B4B4"/>
    
    <SolidColorBrush x:Key="SearchBorder" Color="#E6E6E6"/>
    
    <SolidColorBrush x:Key="ControlBackcolor" Color="#F6F6F6"/>
