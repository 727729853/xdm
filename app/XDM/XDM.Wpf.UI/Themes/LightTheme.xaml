    <SolidColorBrush x:Key="ListViewIconForecolor" Color="DodgerBlue"/>
    <SolidColorBrush x:Key="ListViewBackcolor" Color="White"/>
    <SolidColorBrush x:Key="ListViewForecolor" Color="#696969"/>
    <SolidColorBrush x:Key="ListViewMouseOverBackcolor" Color="#F6F6F6"/>
    <SolidColorBrush x:Key="ListViewMouseOverForecolor" Color="#3C3C3C"/>
    <SolidColorBrush x:Key="ListViewHeaderForecolor" Color="#A0A0A0"/>
    
    <SolidColorBrush x:Key="StatusbarBackcolor" Color="White"/>
    <SolidColorBrush x:Key="StatusbarIconcolor" Color="DodgerBlue"/>
    
    <Thickness x:Key="CategoryListBorderThickness">0</Thickness>
    <Thickness x:Key="MainContentBorderThickness">0,1,0,0</Thickness>
    
    <SolidColorBrush x:Key="PlaceHolderForecolor" Color="#B4B4B4"/>
</ResourceDictionary>